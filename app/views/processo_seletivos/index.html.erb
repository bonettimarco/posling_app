<p id="notice"><%= notice %></p>

<h1>Listing Processos Seletivos</h1>

<table>
  <thead>
    <tr>
      <th>Cabecalho</th>
      <th>Mestrado</th>
      <th>Texto1</th>
      <th>Doutorado</th>
      <th>Texto2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @processo_seletivos.each do |processo_seletivo| %>
      <tr>
        <td><%= processo_seletivo.cabecalho %></td>
        <td><%= processo_seletivo.mestrado %></td>
        <td><%= processo_seletivo.texto1 %></td>
        <td><%= processo_seletivo.doutorado %></td>
        <td><%= processo_seletivo.texto2 %></td>
    		<% if logged_in? %>
          <td><%= link_to 'Show', processo_seletivo %></td>
          <td><%= link_to 'Edit', edit_processo_seletivo_path(processo_seletivo) %></td>
          <td><%= link_to 'Destroy', processo_seletivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
		<% if logged_in? %>
      <%= link_to 'New Processo seletivo', new_processo_seletivo_path %>
    <% end %>