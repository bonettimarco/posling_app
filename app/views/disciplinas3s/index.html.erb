<p id="notice"><%= notice %></p>
<div class="container-fluid cont-estatico">
    <div class="container-fluid ">
        <h1>Lista das Disciplinas da Linha 3</h1>
        
        <table class="table table-responsive table-bordered table-hover">
          <thead>
            <tr>
              <th>Texto</th>
              <th colspan="3"></th>
            </tr>
          </thead>
        
          <tbody>
            <% @disciplinas3s.each do |disciplinas3| %>
              <tr>
                <td><%= disciplinas3.texto %></td>
        		<% if logged_in? %>
                <td><%= link_to 'Show', disciplinas3 %></td>
                <td><%= link_to 'Edit', edit_disciplinas3_path(disciplinas3) %></td>
                <td><%= link_to 'Destroy', disciplinas3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        		<% end %>
        	  </tr>
            <% end %>
          </tbody>
        </table>
        
        <br>
        <% if logged_in? %>
        <%= link_to 'New Disciplinas3', new_disciplinas3_path %>
        <% end %>
    </div>
  </div>  