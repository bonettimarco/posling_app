<p id="notice"><%= notice %></p>
<div class="container-fluid cont-estatico">
    <div class="container-fluid ">
<h1>Docentes</h1>

<table class="table table-responsive table-bordered table-hover">
  <thead>
    <tr>
      <th>Linha</th>
      <th>Nome</th>
      <% if logged_in? %>
      <th colspan="3"></th>
      <% end %>
    </tr>
  </thead>

  <tbody>
    <% @docentes.each do |docente| %>
      <tr>
        <td><%= docente.linha %></td>
        <td><a href=<%= docente.link %>><%= docente.nome %></td>
        <% if logged_in? %>
          <td><%= link_to 'Show', docente %></td>
          <td><%= link_to 'Edit', edit_docente_path(docente) %></td>
          <td><%= link_to 'Destroy', docente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

		<% if logged_in? %>
		  <%= link_to 'New Docente', new_docente_path %>
    <% end %>
    
  </div>
</div>