<p id="notice"><%= notice %></p>

<h1>Listing Publicas</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Texto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publicas.each do |publica| %>
      <tr>
        <td></td>
        <td><a href=<%= publica.texto %>><%= publica.titulo %></td>
    		<% if logged_in? %>
          <td><%= link_to 'Show', publica %></td>
          <td><%= link_to 'Edit', edit_publica_path(publica) %></td>
          <td><%= link_to 'Destroy', publica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
		<% if logged_in? %>
      <%= link_to 'New Publica', new_publica_path %>
    <% end %>