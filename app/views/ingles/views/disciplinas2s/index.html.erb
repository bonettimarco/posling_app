<p id="notice"><%= notice %></p>

<h1>Lista das Disciplinas da Linha 2</h1>

<table>
  <thead>
    <tr>
      <th>Texto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @disciplinas2s.each do |disciplinas2| %>
      <tr>
        <td><%= disciplinas2.texto %></td>
		<% if logged_in? %>
        <td><%= link_to 'Show', disciplinas2 %></td>
        <td><%= link_to 'Edit', edit_disciplinas2_path(disciplinas2) %></td>
        <td><%= link_to 'Destroy', disciplinas2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		<% end %>
	  </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if logged_in? %>
	<%= link_to 'New Disciplinas2', new_disciplinas2_path %>
<% end %>